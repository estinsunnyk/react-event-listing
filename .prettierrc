{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "endOfLine": "lf",
  "bracketSpacing": true,
  "arrowParens": "always",
  "bracketSameLine": true,
  "proseWrap": "preserve",
  "overrides": [
    {
      "files": "*.css",
      "options": {
        "printWidth": 200
      }
    }
  ]
}